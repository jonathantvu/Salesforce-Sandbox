    <validationRules>
        <fullName>Appraisal_Genesis_Required_for_Appr_2_3</fullName>
        <active>true</active>
        <description>If 2nd and 3rd Requirements are created, Appraisal Genesis is required</description>
        <errorConditionFormula>ISPICKVAL( Appraisal_Genesis__c, "")
&amp;&amp;
OR((ISPICKVAL( Requirement__c ,'Appraisal - 2nd')),(ISPICKVAL( Requirement__c,'Appraisal - 3rd')))</errorConditionFormula>
        <errorMessage>Please enter Appraisal Genesis</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Moving_Req_Status_to_In_Progress</fullName>
        <active>false</active>
        <description>Requirement status cannot be changed to In Progress if Success Factor is completed</description>
        <errorConditionFormula>OR( ISPICKVAL(Success_Factor__r.Status__c, "Completed") , 
ISPICKVAL( Status__c , "In Progress"))</errorConditionFormula>
        <errorMessage>Must move Success Factor status to "In Progress" before moving the Requirement status</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Reason_Field_Required</fullName>
        <active>true</active>
        <description>Reason field is required to elaborate on these requirements:
- Escrow Holdback for Item that Must be Repaired
- Must pay outstanding judgements
- Other</description>
        <errorConditionFormula>ISBLANK(Reason__c)
&amp;&amp;
ISPICKVAL(Requirement__c, "Escrow Holdback for Item that Must be Repaired")</errorConditionFormula>
        <errorDisplayField>Reason__c</errorDisplayField>
        <errorMessage>Reason field required. Specify item needing repairs.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Req_cant_be_complete_if_tasks_incomplete</fullName>
        <active>true</active>
        <description>Requirement status cannot be changed to In Progress if Success Factor is completed</description>
        <errorConditionFormula>ISCHANGED(Status__c) &amp;&amp;
Tasks_Cleared_Count__c  &lt;&gt;  Total_Tasks__c &amp;&amp; 
ISPICKVAL(Status__c, "Completed")</errorConditionFormula>
        <errorMessage>Cannot complete Requirement. There are outstanding Tasks.</errorMessage>
    </validationRules>
