        <externalId>false</externalId>
        <formula>AND(
Opportunity__r.Record_Type_Name__c = "HomeBuyer", 
(Property_State__c &lt;&gt; "OR"),
(Property_State__c &lt;&gt; "WA"),

OR(
NOT(ISBLANK(Applicant_Trust_Signature__c)),
